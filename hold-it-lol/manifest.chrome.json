{
  "manifest_version": 3,
  "name": "holdit.lol fresh",
  "version": "0.1",
  "description": "Handy objection.lol Courtroom features",
  "permissions": [
    "webNavigation",
    "storage",
    "tabs",
    "contextMenus",
    "unlimitedStorage",
    "tts",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "*://objection.lol/courtroom/*",
    "*://cdn.discordapp.com/*",
    "*://file.garden/*",
    "*://www.court-records.net/*",
    "*://user-images.githubusercontent.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://objection.lol/courtroom/*"],
      "js": ["content/main.js"],
      "css": ["content/style.css"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://objection.lol/courtroom/*"],
      "js": ["content/ws_patch.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://objection.lol/*"],
      "resources": [
        "inject/vue-wrapper.js",
        "inject/closest-match/closest-match.js",
        "inject/jsZip.min.js",
        "toggle-switch.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true

  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  }
}
